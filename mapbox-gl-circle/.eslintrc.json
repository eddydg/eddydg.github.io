{
    "extends": ["eslint:recommended", "google"],
    "env": {
        "browser": true,
        "commonjs": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {"code": 120, "ignoreStrings": true}],
        "comma-dangle": ["error", "never"],
        "space-before-function-paren": ["warn", {"anonymous": "always", "named": "never"}]
    }
}
